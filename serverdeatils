        listen 80;
        listen [::]:80;

        root /var/www/royalsclub.in/html;
        index index.html index.htm index.nginx-debian.html;

        server_name royalsclub.in www.royalsclub.in;

        location / {
                try_files $uri $uri/ =404;
        }
}


